    "babel-plugin-formatjs": "^10.5.3",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-formatjs": "^4.10.3",
    "eslint-plugin-import": "^2.27.5",
