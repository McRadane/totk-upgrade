{
  "name": "totk-armor-upgrade",
  "private": true,
  "version": "1.4.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lang:extract": "node scripts/lang --extract && node scripts/lang --generate-schema",
    "lang:compile": "node scripts/lang.js --generate-sources",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
