    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-redux": "^8.1.0",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sass": "^1.63.4",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@formatjs/cli-lib": "^6.1.3",
    "@fullhuman/postcss-purgecss": "^5.0.0",
